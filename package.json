{
  "name": "incineroar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest",
    "db:build": "docker build -t incineroar-db-image:$(npm pkg get version | tr -d '\"') -f Dockerfile.db .",
    "db:run": "docker start incineroar-db 2>/dev/null || docker run -d -p 27017:27017 --name incineroar-db incineroar-db-image:$(npm pkg get version | tr -d '\"')",
    "genmocks": "tsx ./scripts/gen-antd-mocks.ts",
    "compile": "npx tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/icons": "^6.1.0",
    "@ant-design/nextjs-registry": "^1.2.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@pkmn/sets": "^5.2.0",
    "@tanstack/react-query": "^5.90.10",
    "antd": "^5.28.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "jose": "^6.1.1",
    "mongoose": "^8.19.3",
    "next": "16.0.1",
    "next-logger": "^5.0.2",
    "pino": "^10.1.0",
    "pokenode-ts": "^1.20.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "core-js": "^3.47.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
